//Problem 3 on Project Euler
//Find the largest prime of 600851475143

#include <iostream>
using namespace std;

int largest_prime=0;
//int num=600851475143;

int main() {
	for(int i=1; i<600851475144; i++) {
	    if(600851475143%i == 0) {
	        largest_prime = i;
	    }
	}
	cout << "The largest prime of 600851475143 is " << largest_prime << endl;
	return 0;
}
